{
    "name": "tscc",
    "version": "1.0.0",
    "type": "module",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "test:unit": "node --test-reporter spec --test build/test/unit",
        "test:feature": "node build/test/features.js",
        "test": "yarn test:unit && yarn test:feature",
        "format": "prettier -w src/*.ts test/*.ts *.json",
        "clean": "rm -rf build"
    },
    "dependencies": {
        "@types/node": "^18.15.0",
        "typescript": "^5.1.3"
    },
    "devDependencies": {
        "prettier": "^2.8.4"
    },
    "engines": {
        "node": ">=19.6.0"
    }
}
